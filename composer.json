{
    "name": "password-cockpit/backend",
    "description": "Password-cockpit backend",
    "type": "project",
    "homepage": "https://github.com/passwordcockpit/backend",
    "license": "BSD-3-Clause",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": false,
            "laminas/laminas-component-installer": false,
            "laminas/laminas-dependency-plugin": false
        }
    },
    "require": {
        "php": "^8.2",
        "doctrine/migrations": "^3.4",
        "doctrine/orm": "^2.11",
        "firebase/php-jwt": "^6.0",
        "guidofaecke/blast-base-url": "^2.1",
        "laminas/laminas-authentication": "^2.5",
        "laminas/laminas-component-installer": "^3.4.0",
        "laminas/laminas-config-aggregator": "^1.1.0",
        "laminas/laminas-crypt": "^3.2",
        "laminas/laminas-db": "^2.9",
        "laminas/laminas-dependency-plugin": "^2.6",
        "laminas/laminas-hydrator": "^4.3",
        "laminas/laminas-i18n": "^2.7",
        "laminas/laminas-i18n-resources": "^2.5",
        "laminas/laminas-inputfilter": "^2.8",
        "laminas/laminas-ldap": "^2.16",
        "laminas/laminas-paginator": "^2.8",
        "laminas/laminas-permissions-rbac": "^3.0",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-stratigility": "^3.0",
        "mezzio/mezzio": "3.19",
        "mezzio/mezzio-hal": "^2.9",
        "mezzio/mezzio-helpers": "^5.16",
        "mezzio/mezzio-laminasrouter": "^3.9",
        "mezzio/mezzio-problem-details": "^1.14",
        "mezzio/mezzio-router": "^3.17",
        "roave/psr-container-doctrine": "^3.4",
        "tuupola/cors-middleware": "^1.2",
        "tuupola/slim-jwt-auth": "dev-php-jwt-6",
        "zircote/swagger-php": "^4.2"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^2.0",
        "codeception/module-rest": "^2.0",
        "laminas/laminas-development-mode": "^3.1",
        "mezzio/mezzio-tooling": "^2.5",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/src/",
            "Authentication\\": "src/Authentication/src/",
            "Authorization\\": "src/Authorization/src/",
            "User\\": "src/User/src",
            "Folder\\": "src/Folder/src",
            "Password\\": "src/Password/src",
            "Log\\": "src/Log/src",
            "File\\": "src/File/src"
        },
        "files": [
            "config/constants.local.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "check": [
            "@cs-check",
            "@test"
        ],
        "clear-config-cache": "php bin/clear-config-cache.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test-debug": [
            "@test-init",
            "sudo php vendor/bin/codecept run --debug",
            "@test-clear"
        ],
        "test": [
            "@test-init",
            "sudo php vendor/bin/codecept run",
            "@test-clear"
        ],
        "swagger": [
            "./vendor/bin/openapi -o ./swagger/swagger.json -e vendor src/ -l"
        ]
    }
}
